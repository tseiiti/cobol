//SYSLMOD  DD DSN=FUTURE.LMODUL.COBOL(PU8EXE03),DISP=SHR
//SYSUT1   DD  UNIT=SYSDA,SPACE=(TRK,(10,10))
//STEPLIB  DD  DSNAME=&LIBPRFX..SCEERUN,DISP=SHR
//SYSPRINT DD  SYSOUT=A
//CEEDUMP  DD  SYSOUT=*
//SYSUDUMP DD  SYSOUT=*
